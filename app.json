{
  "expo": {
    "name": "AwesomeProject",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.goit.awesomeproject",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": {
      "expo-image-picker": {
        "photoLibraryPermission": {
          "read": true,
          "write": true
        }
      },
      "expo-camera": {
        "cameraPermission": "Allow $(AwesomeProject) to access your camera."
      },
      "expo-location": {
        "locationAlwaysAndWhenInUsePermission": "Allow $(AwesomeProject) to use your location."
      }
    }
  }
}
